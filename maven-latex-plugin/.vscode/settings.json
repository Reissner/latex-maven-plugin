{
  "maven.view": "hierarchical",
  "java.test.config": {
    "name": "myTestConfig",
    "vmArgs": [
      "-ea",
      "-esa",
      "-Xverify:all",
      "-Xmx1023m",
      "-Denvironment=${workspaceFolder}/",
      "-Dtarget=${workspaceFolder}/target/",
      "-DsrcJavaDir=src/main/java/",
      "-DsrcAntlr4Dir=src/main/antlr4/",
      "-Deu.simuline.octave.executable=octave",
      "-DmCompilerProps=${workspaceFolder}/src/main/resources/package/inst/compiler.props"
    ]
  },
  "java.test.defaultConfig": "myTestConfig",
  "java.compile.nullAnalysis.mode": "automatic",
  "java.configuration.updateBuildConfiguration": "interactive",
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx2G -Xms100m -Xlog:disable",
  "latex-workshop.view.pdf.external.synctex.command": "okular",
  "latex-workshop.view.pdf.external.synctex.args": [
      "--unique",
      "%PDF%#src:%LINE%%TEX%"
  ]
}